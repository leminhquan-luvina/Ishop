	<clabel id="txtCompanyAddress" text=""  PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="3" gridy="0"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="1.0f" weighty="0.0f" />
	</clabel>	
	<!-- Tel - Fax -->
	<clabel id="lblCompanyTelephone" text="$langPack.getString('company.telephone')"  PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
