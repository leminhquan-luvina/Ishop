		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="1" gridy="1"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<clabel id="lblCompanyFax" text="$langPack.getString('company.fax')"  PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="2" gridy="1"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<clabel id="txtCompanyFax" text="" PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="3" gridy="1"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="9.0f" weighty="0.0f" />
	</clabel>
	<!-- Nguoi lien he - ngay thanh lap -->
	<clabel id="lblContactPerson" text="$langPack.getString('company.contact.person')" PreferredSize="100,25"  >
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="0" gridy="2"  insets="5,5,15,5"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<clabel id="txtContactPerson" PreferredSize="100,25"  >
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="1" gridy="2"  insets="5,5,15,5"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
