							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="2" gridy="2"  insets="5,5,15,5"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<clabel id="dcestablishedDate" PreferredSize="100,25"  >
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="3" gridy="2"  insets="5,5,15,5"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<cwhitepanel id="companyInformationActionPanel" layout="FlowLayout(FlowLayout.CENTER,5,5)">
		<gridbagconstraints Anchor="GridBagConstraints.CENTER" 
				refid="company_infor_gbc" ipadx="0" ipady="0"
				gridx="0" gridy="3"  insets="3,3,3,3"  
				gridwidth="4" gridheight="1"
				Fill="GridBagConstraints.HORIZONTAL" weightx="1.0f" weighty="0.0f" />
		<!-- <cdlgbutton id="companyInforSaveButton" text="$langPack.getString('buttonSave')"  
					PreferredSize="73,22" MaximumSize="73,22" MinimumSize="73,22"/> -->
		<!-- cdlgbutton id="warehouseInforNewButton" text="$langPack.getString('buttonNew')"   
				PreferredSize="73,22" MaximumSize="73,22" MinimumSize="73,22"/-->
	</cwhitepanel>
	<cwhitepanel id="companyFieldListActionPanel" layout="FlowLayout(FlowLayout.LEFT,5,5)">
		<gridbagconstraints Anchor="GridBagConstraints.CENTER" 
				refid="company_infor_gbc" ipadx="0" ipady="0"
				gridx="0" gridy="4"  insets="3,3,3,3"  
				gridwidth="4" gridheight="1"
				Fill="GridBagConstraints.HORIZONTAL" weightx="1.0f" weighty="0.0f" />
		<clabel id="lblFieldName" text="$langPack.getString('company.business.field.name')" PreferredSize="180,25"  />
		<ctextfield id="txtFieldName" PreferredSize="250,22" MaximumSize="250,22" MinimumSize="73,22"></ctextfield>
		<cdlgbutton id="saveButton" text="$langPack.getString('buttonSave')"  
					PreferredSize="73,22" MaximumSize="73,22" MinimumSize="73,22"
					action="SAVE_BUSINESS_FIELD"/>
	</cwhitepanel>
	<cpaging id="businessFieldPaging">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START"
			refid="po_detail_gbc1" ipadx="0" ipady="0" gridx="0" gridy="5"
			insets="3,3,3,3" gridwidth="4" gridheight="1" Fill="GridBagConstraints.BOTH"
			weightx="1.0f" weighty="1.0f" />
	</cpaging>
</cwhitepanel>