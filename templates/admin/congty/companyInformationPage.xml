<cwhitepanel Layout="GridBagLayout" id="companyInformationPanel">
	<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
								refid="company_infor_gbc_container" ipadx="0" ipady="0"
								 gridx="0" gridy="0"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
								Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	<!-- Ten congty - Cong ty -->
	<clabel id="lblCompanyName" text="$langPack.getString('company.name')"   PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="0" gridy="0"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />
	</clabel>
	<clabel id="txtCompanyName" text="" PreferredSize="300,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
							 gridx="1" gridy="0"  insets="3,3,3,3"  gridwidth="1" gridheight="1"
							Fill="GridBagConstraints.HORIZONTAL" weightx="1.0f" weighty="0.0f" />
	</clabel>
	<clabel id="lblCompanyAddress" text="$langPack.getString('company.address')"   PreferredSize="100,25">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
							refid="company_infor_gbc" ipadx="0" ipady="0"
