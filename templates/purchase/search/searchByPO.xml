<?xml version="1.0" encoding="UTF-8"?>
<cwhitepanel id="billSearchPanel" Layout="GridBagLayout">
<!-- Line loai phieu -->
	<clabel id="lblPONumber" text="$langPack.getString('purchase.search.po.number')">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
				refid="purchase_search_gbc5" ipadx="0" ipady="0"
			 	gridx="0" gridy="0"  insets="5,5,5,5"  gridwidth="1" gridheight="1"
				Fill="GridBagConstraints.HORIZONTAL" weightx="0.0f" weighty="0.0f" />	
	</clabel>
	<ctextfield id="txtPOText" PreferredSize="130,23" MaximumSize="130,23" MinimumSize="130,23">
		<gridbagconstraints Anchor="GridBagConstraints.FIRST_LINE_START" 
				refid="purchase_search_gbc5" ipadx="0" ipady="0"
			 	gridx="1" gridy="0"  insets="5,5,5,5"  gridwidth="1" gridheight="1"
				Fill="GridBagConstraints.HORIZONTAL" weightx="1.0f" weighty="0.0f" />	
	</ctextfield>
	<cdlgbutton id="purchaseSearchButton4"  text="$langPack.getString('submit.search.button')" 
			action ="saveButtonAction" PreferredSize="73,22" MaximumSize="73,22" MinimumSize="73,22">
		<gridbagconstraints Anchor="GridBagConstraints.CENTER" 
				refid="purchase_search_gbc5" ipadx="0" ipady="0"
			 	gridx="0" gridy="1"  insets="5,5,5,5"  gridwidth="2" gridheight="1"
				Fill="GridBagConstraints.NONE" weightx="1.0f" weighty="0.0f" />
	</cdlgbutton>
	<clabel id="space1" text="">
		<gridbagconstraints Anchor="GridBagConstraints.CENTER" 
				refid="purchase_search_gbc5" ipadx="0" ipady="0"
			 	gridx="0" gridy="3"  insets="5,5,5,5"  gridwidth="2" gridheight="1"
				Fill="GridBagConstraints.NONE" weightx="1.0f" weighty="0.0f" />
	</clabel>
	
</cwhitepanel>